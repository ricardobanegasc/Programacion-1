// funcionFAC.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;


int FAC(int n);

void main()
{
	int n, FAC;
	cout<<"ingrese un numero: " ;
	cin>>n;
	FAC=1;
	for (int i=1;i<=n;i++)
		FAC=FAC*i;
	cout<<"El factorial de "<<n<<" es "<<FAC;
	getch();
}

int FAC(int numero)
{
	int x;
	x=1;
	for (int i=1;i<=numero;i++)
		x=x*i;
	return x;
	getch();
}
